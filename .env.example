# Candlebar Configuration
# Copy this file to .env and update with your values

# App Configuration
VITE_APP_NAME=Candlebar
VITE_APP_VERSION=1.0.0

# Default refresh interval in seconds (10-300)
VITE_DEFAULT_REFRESH_INTERVAL=30

# Feature flags
VITE_ENABLE_CUSTOM_APIS=true
VITE_ENABLE_DRAG_DROP=true

# Default stocks (comma-separated)
VITE_DEFAULT_STOCKS=AAPL,GOOGL,MSFT,TSLA

# Maximum number of stocks allowed
VITE_MAX_STOCKS=20

# Logging level (debug, info, warn, error)
VITE_LOG_LEVEL=info